{
  "name": "@mgn901/m13tk-react",
  "version": "1.1.0",
  "description": "Useful React components and hooks working with m13TK.",
  "entry": "src/index.ts",
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "style": "css/style.css",
  "files": [
    "package.json",
    "LICENSE.txt",
    "README.md",
    "cjs",
    "esm",
    "css"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:esm": "tsc -p ./tsconfig.esm.json",
    "build:cjs": "tsc -p ./tsconfig.cjs.json",
    "watch:esm": "tsc -w -p ./tsconfig.esm.json",
    "watch:cjs": "tsc -w -p ./tsconfig.cjs.json",
    "build:css": "sass ./src/style.scss --style compressed --no-source-map | node ./scripts/sanitizeCSS.js > ./css/style.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mgn901/m13tk-react.git"
  },
  "author": "mgn901 <mgn901+dev@gmail.com> (https://mgn901.pages.dev/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mgn901/m13tk-react/issues"
  },
  "homepage": "https://github.com/mgn901/m13tk-react#readme",
  "peerDependencies": {
    "@mgn901/m13tk": "^2.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-transition-group": "^4.4.2"
  },
  "devDependencies": {
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "@types/react-transition-group": "^4.4.3",
    "sass": "^1.42.1",
    "typescript": "^4.4.3"
  }
}
