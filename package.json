{
  "name": "@mgn901/m13tk-react",
  "version": "1.0.0",
  "description": "Useful React components and hooks working with m13TK.",
  "entry": "src/index.ts",
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "style": "css/style.css",
  "files": [
    "package.json",
    "LICENSE.txt",
    "cjs",
    "es",
    "css"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:esm": "tsc -p ./tsconfig.esm.json",
    "build:cjs": "tsc -p ./tsconfig.cjs.json",
    "build:css": "sass ./src/style.scss --style compressed --no-source-map | node ./scripts/sanitizeCSS.js > ./css/style.css",
    "start": "rollup -c -w --environment BUILD:development",
    "generateThirdpartyFile": "license-checker --production --json --customPath ./scripts/customPath.json | node ./scripts/sanitizeThirdpartyFile.js > ./THIRDPARTY.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mgn901/m13tk-react.git"
  },
  "author": "mgn901 <mgn901+dev@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mgn901/m13tk-react/issues"
  },
  "homepage": "https://github.com/mgn901/m13tk-react#readme",
  "peerDependencies": {
    "@mgn901/m13tk": "^1.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-transition-group": "^4.4.2"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-node-resolve": "^13.0.5",
    "@rollup/plugin-replace": "^3.0.0",
    "@rollup/plugin-typescript": "^8.2.5",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "@types/react-transition-group": "^4.4.3",
    "rollup": "^2.57.0",
    "rollup-plugin-scss": "^3.0.0",
    "rollup-plugin-serve": "^1.1.0",
    "sass": "^1.42.1",
    "typescript": "^4.4.3"
  }
}
